                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[선별기 불량수율, 선별기 불량수량]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[캡슐, 바디 인쇄 위치 불량 비율]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MTRLNAME" Header="[캡슐, 캡 인쇄 지워짐 불량 비율]" Binding="{Binding MTRLNAME}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="BATCHNO" Header="[캡슐, 캡 인쇄 위치 불량 비율]" Binding="{Binding BATCHNO}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[캡슐, 캡 미인쇄 불량 비율]" Binding="{Binding INSPECTIONDTTM}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[캡슐, 바디 탑 영역 불량 비율]" Binding="{Binding INSPECTQTY}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[캡슐, 바디 탑 영역 어두운 불량 비율]" Binding="{Binding TOTAL_GOODQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[캡슐, 캡 탑 영역 불량 비율]" Binding="{Binding GOODYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[캡슐, 캡 탑 영역 어두운 불량 비율]" Binding="{Binding TOTAL_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
