                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[설비명, 설비명]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
