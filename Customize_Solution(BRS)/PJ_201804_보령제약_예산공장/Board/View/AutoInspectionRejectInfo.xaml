<UserControl    x:Class="Board.AutoInspectionRejectInfo"
                xmlns:local="clr-namespace:Board" 
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:icommon="clr-namespace:LGCNS.iPharmMES.Common;assembly=LGCNS.iPharmMES.Common"  
                xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
                xmlns:ControlsLib="clr-namespace:LGCNS.EZMES.ControlsLib;assembly=LGCNS.EZMES.ControlsLib"
                xmlns:Ctrl="clr-namespace:LGCNS.iPharmMES.Controls;assembly=LGCNS.iPharmMES.Controls"
                xmlns:c1="clr-namespace:C1.Silverlight.DataGrid;assembly=C1.Silverlight.DataGrid.5"
                mc:Ignorable="d"
                d:DesignHeight="800" d:DesignWidth="1790" 
                Name="Main">

    <UserControl.Resources>
        <ResourceDictionary>
            <Style x:Key="lblStyle" TargetType="sdk:Label">
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
            </Style>
            <Style x:Key="txtStyle" TargetType="TextBox">
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
                <Setter Property="Width" Value="200"/>
            </Style>
            <Style x:Key="colHeader" TargetType="c1:DataGridColumnHeaderPresenter">
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
            </Style>
            <Style x:Key="radioStyle" TargetType="RadioButton">
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
                <Setter Property="Margin" Value="2 0 2 0"/>
            </Style>
            <local:AutoInspectionRejectInfoViewModel x:Key="ViewModel"/>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot" DataContext="{StaticResource ViewModel}">
        <i:Interaction.Triggers>
            <i:EventTrigger EventName="Loaded">
                <i:InvokeCommandAction Command="{Binding LoadedCommandAsync}"
                                       CommandParameter="{Binding ElementName=Main}"/>
            </i:EventTrigger>
        </i:Interaction.Triggers>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"  />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <ControlsLib:MainPanel_Top x:Name="mainPanel" Grid.Row="1" Grid.RowSpan="1" IsExpanded="True" IsEnableSearch="False" IsEnableSave="False" HorizontalAlignment="Stretch">
            <ControlsLib:MainPanel_Top.Search>
                <Grid x:Name="searchPanel" VerticalAlignment="Center"  Margin="0 5 0 0" Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <sdk:Label Grid.Column="1" Content="선별기 선택" Style="{StaticResource lblStyle}"/>
                    <ComboBox Grid.Column="3" x:Name="EQPTID" Width="140"
                              ItemsSource="{Binding Path=BR_BRS_SEL_AUTO_INSPECTION.OUTDATAs}"
                              DisplayMemberPath="EQPTID" SelectedValuePath="EQPTID"
                              SelectedValue="{Binding Path=Eqptid, Mode=TwoWay}"/>
                    <sdk:Label Grid.Column="5" Content="날짜" Style="{StaticResource lblStyle}"/>
                    <StackPanel Grid.Column="7" Orientation="Horizontal">
                        <Ctrl:iPharmMESFromToCalendar SelectedDateFrom="{Binding Path=PeriodSTDTTM, Mode=TwoWay}"
                                                       SelectedDateTo="{Binding Path=PeriodEDDTTM, Mode=TwoWay}" />
                    </StackPanel>

                    <sdk:Label Grid.Column="9" Content="제품코드" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="11"  Text="{Binding MtrlId, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <sdk:Label Grid.Column="13" Content="제품명" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="15"  Text="{Binding MtrlName, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <sdk:Label Grid.Column="17" Content="제조번호" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="19" Text="{Binding BatchNo, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>

                    <Button Grid.Column="21" Content="검색" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10,0,0,0" Width="80" >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding Path=BtnSearchCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
            </ControlsLib:MainPanel_Top.Search>
            <ControlsLib:MainPanel_Top.Contents>
                <Grid>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="5" />

                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="5" />
                        </Grid.RowDefinitions>

                        <c1:C1DataGrid Grid.Row="1" Grid.Column="1" x:Name="OAIM2001"
                                       HeadersVisibility="None"
                                       AutoGenerateColumns="False"
                                       SelectionMode="MultiRow"
                                       CanUserAddRows="False"                                    
                                       CanUserSort="True" 
                                       ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                       ScrollViewer.VerticalScrollBarVisibility="Auto"
                                       ScrollMode="RealTime"
                                       Visibility="{Binding OAIM2001}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_AUTO_INSPECTION_REJCET_INFO.OUTDATAs, Mode=OneWay}">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="BATCHNO" Header="[배치번호]" Binding="{Binding BATCHNO}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="EQPTID" Header="[설비명]" Binding="{Binding EQPTID}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[선별기 양품수량]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[선별기 불량수량]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[카메라 모서리 영역 불량률]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MTRLNAME" Header="[카메라 각인 지워짐 불량률]" Binding="{Binding MTRLNAME}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="BATCHNO" Header="[카메라 일반 영역 불량률]" Binding="{Binding BATCHNO}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[3D 카메라 정제 조각 불량률]" Binding="{Binding INSPECTIONDTTM}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[미분류된 불량률]" Binding="{Binding INSPECTQTY}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[색상 불량률]" Binding="{Binding TOTAL_GOODQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[정면 카메라 색얼룩 불량률]" Binding="{Binding GOODYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[정면 카메라 이물 불량률]" Binding="{Binding TOTAL_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[정면 카메라 모서리 이물 불량률]" Binding="{Binding REJECTYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[정면 카메라 모서리 영역 얼룩 불량률]" Binding="{Binding CRITICAL_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[정면 카메라 얼룩 불량률]" Binding="{Binding CRITICALYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[인쇄 지워짐 불량률]" Binding="{Binding MIDDLE_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[인쇄 번짐 불량률]" Binding="{Binding MIDDLEYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[측면 카메라 색얼룩 불량률]" Binding="{Binding LIGHT_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[측면 카메라 이물 불량률]" Binding="{Binding LIGHTYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO1" Header="[측면 카메라 얼룩 불량률]" Binding="{Binding REJECT_NO1}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO2" Header="[두꺼운 정제 불량률]" Binding="{Binding REJECT_NO2}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO3" Header="[얇은 정제 불량률]" Binding="{Binding REJECT_NO3}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                            <c1:C1DataGrid.TopRows>
                                <c1:DataGridColumnHeaderRow />
                            </c1:C1DataGrid.TopRows>
                        </c1:C1DataGrid>
                        
                        <c1:C1DataGrid Grid.Row="1" Grid.Column="1" x:Name="OAIM2003_T"
                                       HeadersVisibility="None"
                                       AutoGenerateColumns="False"
                                       SelectionMode="MultiRow"
                                       CanUserAddRows="False"                                    
                                       CanUserSort="True" 
                                       ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                       ScrollViewer.VerticalScrollBarVisibility="Auto"
                                       ScrollMode="RealTime"
                                       Visibility="{Binding OAIM2003_T}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_UDT_BRS_SVP_REJECT_INFO.OUTDATAs, Mode=OneWay}">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[배치번호]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[설비명]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[선별기 양품수량]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[선별기 불량수량]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[[정제] 3D 카메라 모서리 영역 불량 비율]" Binding="{Binding INSUSER}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MTRLNAME" Header="[[정제] 3D 카메라 각인 지워짐 불량 비율]" Binding="{Binding MTRLNAME}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="BATCHNO" Header="[[정제] 3D 카메라 일반 영역 불량 비율]" Binding="{Binding BATCHNO}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[[정제] 3D 카메라 정제 조각 불량 비율]" Binding="{Binding INSPECTIONDTTM}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[[정제] 색상 불량 비율]" Binding="{Binding INSPECTQTY}" HorizontalAlignment="Center" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[[정제] 정면 카메라 색얼룩 불량 비율]" Binding="{Binding TOTAL_GOODQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSUSER" Header="[[정제] 정면 카메라 이물 불량 비율]" Binding="{Binding GOODYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[[정제] 정면 카메라 모서리 이물 불량 비율]" Binding="{Binding TOTAL_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[[정제] 정면 카메라 모서리 영역 얼룩 불량 비율]" Binding="{Binding REJECTYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[[정제] 정면 카메라 얼룩 불량 비율]" Binding="{Binding CRITICAL_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[[정제] 미검사 종료 비율]" Binding="{Binding CRITICALYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[[정제] 앞뒷면 불일치 비율]" Binding="{Binding MIDDLE_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTIONDTTM" Header="[[정제] 미인쇄 비율]" Binding="{Binding MIDDLEYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[[정제] 인쇄 지워짐 불량 비율]" Binding="{Binding LIGHT_REJECTQTY}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTQTY" Header="[[정제] 인쇄 번짐 불량 비율]" Binding="{Binding LIGHTYIELD}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO1" Header="[[정제] 측면 카메라 색얼룩 불량 비율]" Binding="{Binding REJECT_NO1}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO2" Header="[[정제] 측면 카메라 이물 불량 비율]" Binding="{Binding REJECT_NO2}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO3" Header="[[정제] 측면 카메라 모서리 영역 불량 비율]" Binding="{Binding REJECT_NO3}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO3" Header="[[정제] 측면 카메라 얼룩 불량 비율]" Binding="{Binding REJECT_NO3}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO3" Header="[[정제] 두꺼운 정제 불량 비율]" Binding="{Binding REJECT_NO3}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="REJECT_NO3" Header="[[정제] 얇은 정제 불량 비율]" Binding="{Binding REJECT_NO3}" HorizontalAlignment="Right" VerticalAlignment="Center" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
