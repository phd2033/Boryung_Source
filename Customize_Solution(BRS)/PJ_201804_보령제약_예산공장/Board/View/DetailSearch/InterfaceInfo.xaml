<UserControl    x:Class="Board.InterfaceInfo"
                xmlns:local="clr-namespace:Board" 
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:icommon="clr-namespace:LGCNS.iPharmMES.Common;assembly=LGCNS.iPharmMES.Common"  
                xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
                xmlns:ControlsLib="clr-namespace:LGCNS.EZMES.ControlsLib;assembly=LGCNS.EZMES.ControlsLib"
                xmlns:Ctrl="clr-namespace:LGCNS.iPharmMES.Controls;assembly=LGCNS.iPharmMES.Controls"
                xmlns:c1="clr-namespace:C1.Silverlight.DataGrid;assembly=C1.Silverlight.DataGrid.5"
                mc:Ignorable="d"
                d:DesignHeight="768" d:DesignWidth="1024" 
                Name="Main">

    <UserControl.Resources>
        <ResourceDictionary>
            <Style x:Key="lblStyle" TargetType="sdk:Label">
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
            </Style>
            <Style x:Key="txtStyle" TargetType="TextBox">
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
                <Setter Property="Width" Value="200"/>
            </Style>
            <Style x:Key="colHeader" TargetType="c1:DataGridColumnHeaderPresenter">
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
            </Style>
            <Style x:Key="radioStyle" TargetType="RadioButton">
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="Malgun Gothic"/>
                <Setter Property="FontSize" Value="12"/>
                <Setter Property="Margin" Value="2 0 2 0"/>
            </Style>
            <local:InterfaceInfoViewModel x:Key="ViewModel"/>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot" DataContext="{StaticResource ViewModel}">
        <i:Interaction.Triggers>
            <i:EventTrigger EventName="Loaded">
                <i:InvokeCommandAction Command="{Binding LoadedCommand}"
                                       CommandParameter="{Binding ElementName=Main}"/>
            </i:EventTrigger>
        </i:Interaction.Triggers>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"  />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <ControlsLib:MainPanel_Top x:Name="mainPanel" Grid.Row="1" Grid.RowSpan="1" IsExpanded="True" IsEnableSearch="False" IsEnableSave="False" HorizontalAlignment="Stretch">
            <ControlsLib:MainPanel_Top.Search>
                <Grid x:Name="searchPanel" VerticalAlignment="Center"  Margin="0 5 0 0" Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="10" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="100" />
                    </Grid.ColumnDefinitions>
                    <ComboBox Grid.Column="1" Width="150" SelectedValue="{Binding SelectedMode,Mode=TwoWay}" SelectedValuePath="Tag">
                        <ComboBoxItem Content="SAP 오더 정보" Tag="SAP_Order"/>
                        <ComboBoxItem Content="SAP 라우터 정보" Tag="SAP_Route"/>
                        <ComboBoxItem Content="TnT 실적 정보" Tag="TnT"/>
                        <ComboBoxItem Content="WMS 피킹 정보" Tag="WMS"/>
                    </ComboBox>
                    <sdk:Label Grid.Column="3" Content="계획시작일자" Style="{StaticResource lblStyle}"/>
                    <StackPanel Grid.Column="5" Orientation="Horizontal" Width="210">
                        <Ctrl:iPharmMESFromToCalendar SelectedDateFrom="{Binding Path=PeriodSTDTTM, Mode=TwoWay}"
                                                       SelectedDateTo="{Binding Path=PeriodEDDTTM, Mode=TwoWay}"/>
                    </StackPanel>
                    <sdk:Label Grid.Column="7" Content="오더번호" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="9" Text="{Binding POID, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <sdk:Label Grid.Column="11" Content="배치번호" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="13" Text="{Binding BATCHNO, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <sdk:Label Grid.Column="15" Content="제품코드" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="17"  Text="{Binding MTRLID, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <sdk:Label Grid.Column="19" Content="제품명" Style="{StaticResource lblStyle}"/>
                    <TextBox Grid.Column="21" Text="{Binding MTRLNAME, Mode=TwoWay}" Style="{StaticResource txtStyle}"/>
                    <Button Grid.Column="22" Content="검색" Width="80" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding Path=BtnSearchCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
            </ControlsLib:MainPanel_Top.Search>
            <ControlsLib:MainPanel_Top.Contents>
                <Grid>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="5" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="5" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="1" Grid.Column="1" Text="오더 정보" FontWeight="Bold" FontSize="14" Visibility="{Binding sapOrder}"/>
                        <c1:C1DataGrid Grid.Row="3" Grid.Column="1" x:Name="dgProductionOrder"
                                       AutoGenerateColumns="False"
                                       HeadersVisibility="Column"
                                       SelectionMode="SingleRow"
                                       CanUserAddRows="False"
                                       CanUserSort="True"
                                       Visibility="{Binding sapOrder}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_SAP_R_ProductionOrderInfo.OUTDATAs, Mode=OneWay}">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding SelectionChangedCommand}"
                                                           CommandParameter="{Binding ElementName=dgProductionOrder, Path=SelectedItem}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="AUFNR" Header="오더번호" Binding="{Binding AUFNR}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="CHARG" Header="배치번호" Binding="{Binding CHARG}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ZCHARG" Header="수탁사 제조번호" Binding="{Binding ZCHARG}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="WERKS" Header="플랜트" Binding="{Binding WERKS}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MATNR" Header="제품코드" Binding="{Binding MATNR}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="KTEXT" Header="제품명" Binding="{Binding KTEXT}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PSMNG" Header="오더수량" Binding="{Binding PSMNG}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MEINS" Header="기본단위" Binding="{Binding MEINS}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PLNNR" Header="PLNNR" Binding="{Binding PLNNR}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PLNAL" Header="PLNAL" Binding="{Binding PLNAL}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>
                        <TextBlock Grid.Row="5" Grid.Column="1" Text="오더 상세 정보" FontWeight="Bold" FontSize="14" Visibility="{Binding sapOrder}"/>
                        <c1:C1DataGrid Grid.Row="7" Grid.Column="1" x:Name="dgDetail"
                                       AutoGenerateColumns="False"
                                       HeadersVisibility="Column"
                                       CanUserAddRows="False"
                                       CanUserSort="True"
                                       Visibility="{Binding sapOrder}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_SAP_R_ProductionOrderDetail.OUTDATAs, Mode=OneWay}">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding SelectionChangedCommand}"
                                                           CommandParameter="{Binding ElementName=dgDetail, Path=SelectedItem}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="OPSGNAME" Header="오더번호" Binding="{Binding AUFNR}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="WERKS" Header="플랜트" Binding="{Binding WERKS}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="LTXA1" Header="공정" Binding="{Binding LTXA1}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ARBPL" Header="작업장" Binding="{Binding ARBPL}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="BEARZ" Header="인원공수(분)" Binding="{Binding BEARZ}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ERDAT" Header="생성일" Binding="{Binding ERDAT}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ERZET" Header="생성시간" Binding="{Binding ERZET}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="STATUS" Header="상태" Binding="{Binding STATUS}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="IF_RSLT_CODE" Header="결과" Binding="{Binding IF_RSLT_CODE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSDTTM" Header="날짜" Binding="{Binding INSDTTM}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>
                        <TextBlock Grid.Row="9" Grid.Column="1" Text="오더 BOM 정보" FontWeight="Bold" FontSize="14" Visibility="{Binding sapOrder}"/>
                        <c1:C1DataGrid Grid.Row="11" Grid.Column="1" x:Name="dgDetail2"
                                       AutoGenerateColumns="False"
                                       HeadersVisibility="Column"
                                       CanUserAddRows="False"
                                       CanUserSort="True"
                                       Visibility="{Binding sapOrder}"
                                       ItemsSource="{Binding Path=BR_PHR_RPT_ProductionOrderDetail_Result.OUTDATAs, Mode=OneWay}">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="OPSGNAME" Header="공정명" Binding="{Binding OPSGNAME}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ACTSTDATE" Header="시작일" Binding="{Binding ACTSTDATE}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ACTEDDATE" Header="종료일" Binding="{Binding ACTEDDATE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="RECEIVEDQTY" Header="인수량" Binding="{Binding RECEIVEDQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="RECEIVEDQTY_UOM" Header="인수량 단위" Binding="{Binding RECEIVEDQTY_UOM}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PRODQTY" Header="생산량" Binding="{Binding PRODQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PRODQTY_UOM" Header="생산량 단위" Binding="{Binding PRODQTY_UOM}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="SCRAPQTY" Header="불량" Binding="{Binding SCRAPQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="SAMPLEQTY" Header="샘플량" Binding="{Binding SAMPLEQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="STDYIELD" Header="기준수율" Binding="{Binding STDYIELD}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="ACTYIELD" Header="수율" Binding="{Binding ACTYIELD}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>

                        <TextBlock Grid.Row="1" Grid.Column="1" Name="TEST" Text="실적 정보" FontWeight="Bold" FontSize="14" Visibility="{Binding tnt}">
                        </TextBlock>
                        <c1:C1DataGrid Grid.Row="3" Grid.Column="1" x:Name="dgProductionOrder2"
                                       AutoGenerateColumns="False"
                                       HeadersVisibility="Column"
                                       SelectionMode="SingleRow"
                                       CanUserAddRows="False"
                                       CanUserSort="True"
                                       Visibility="{Binding tnt}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_TnTOutputInfoList.OUTDATAs, Mode=OneWay}">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding SelectionChangedCommand}"
                                                           CommandParameter="{Binding ElementName=dgProductionOrder, Path=SelectedItem}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="POID" Header="오더번호" Binding="{Binding POID}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="BATCHNO" Header="배치번호" Binding="{Binding BATCHNO}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MTRLID" Header="제품코드" Binding="{Binding MTRLID}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="MTRLNAME" Header="제품명" Binding="{Binding MTRLNAME}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PBARCODE" Header="파마코드" Binding="{Binding PBARCODE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="PALLETID" Header="용기번호" Binding="{Binding PALLETID}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="OUTPUTQTY" Header="실적수량" Binding="{Binding OUTPUTQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="TRANS_DATE" Header="전송날짜" Binding="{Binding TRANS_DATE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="IS_LASTPALLET" Header="마지막 팔레트 여부" Binding="{Binding IS_LASTPALLET}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSDTTM" Header="작업날짜" Binding="{Binding INSDTTM}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>

                        <TextBlock Grid.Row="5" Grid.Column="1" Text="최종 실적 정보" FontWeight="Bold" FontSize="14" Visibility="{Binding tnt}">
                        </TextBlock>

                        <c1:C1DataGrid Grid.Row="7" Grid.Column="1" x:Name="dgDetail3" Tag="TEST"
                                       AutoGenerateColumns="False"
                                       HeadersVisibility="Column"
                                       CanUserAddRows="False"
                                       CanUserSort="True"
                                       Visibility="{Binding tnt}"
                                       ItemsSource="{Binding Path=BR_BRS_SEL_TnTWorkOrderComplete.OUTDATAs, Mode=OneWay}">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn SortMemberPath="POID" Header="오더번호" Binding="{Binding POID}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="OUTPUTQTY" Header="최종 실적수량" Binding="{Binding OUTPUTQTY}" IsReadOnly="True"  HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="SAMPLEQTY" Header="최종 샘플 수량" Binding="{Binding SAMPLEQTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="STORAGE_SAMPLE_QTY" Header="보관검체" Binding="{Binding STORAGE_SAMPLE_QTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="INSPECTION_SAMPLE_QTY" Header="실험검체" Binding="{Binding INSPECTION_SAMPLE_QTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="GENERAL_SAMPLE_QTY" Header="일반검체" Binding="{Binding GENERAL_SAMPLE_QTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="STABILITY_SAMPLE_QTY" Header="안전검체" Binding="{Binding STABILITY_SAMPLE_QTY}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="IF_RSLT_CODE" Header="처리결과" Binding="{Binding IF_RSLT_CODE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="IF_RSLT_MSG" Header="비고" Binding="{Binding IF_RSLT_MSG}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="TRANS_TP" Header="상태" Binding="{Binding TRANS_TP}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                                <c1:DataGridTextColumn SortMemberPath="TRANS_DATE" Header="전송일자" Binding="{Binding TRANS_DATE}" IsReadOnly="True" HeaderStyle="{StaticResource colHeader}"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>


                    </Grid>
                </Grid>
            </ControlsLib:MainPanel_Top.Contents>
        </ControlsLib:MainPanel_Top>
    </Grid>
</UserControl>
